{
  "ConnectionStrings": {
    "Events": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.BuildingRegistry;Trusted_Connection=True;TrustServerCertificate=True;"
  },

  "BaseUrl": "https://api.staging-basisregisters.vlaanderen/",
  "TicketingUrl": "",
  "NotificationTopicArn": "",

  "DistributedLock": {
    "Region": "eu-west-1",
    "TableName": "__DistributedLocks__",
    "LeasePeriodInMinutes": 5,
    "ThrowOnFailedRenew": true,
    "TerminateApplicationOnFailedRenew": true,
    "Enabled": true
  },

  "SqsQueueUrl": "",

  "SqsRateLimit": {
    "OfficeHoursStartHour": 7,
    "OfficeHoursEndHour": 19,
    "MaxMessagesPerSecondOfficeHours": 1,
    "MaxMessagesPerSecondOutsideOfficeHours": 3
  },

  "Tools": {
    "EnableRepairBuilding": true
  },

  "Cors": [
    "http://localhost:3000",
    "http://localhost:5000"
  ],

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Properties": {
      "Application": "BuildingRegistry - Tools Console",
      "ContainerId": "REPLACE_CONTAINERID"
    }
  }
}
