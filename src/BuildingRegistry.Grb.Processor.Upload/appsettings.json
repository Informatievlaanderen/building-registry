{
  "ConnectionStrings": {
    "BuildingGrb": "Server=(localdb)\\mssqllocaldb;Database=EFProviders.InMemory.BuildingRegistryGrb;Trusted_Connection=True;TrustServerCertificate=True;"
  },

  "BucketName": "",
  "TicketingUrl": "",

  "EcsTaskOptions": {
    "TaskDefinition": "",
    "Cluster": ""
  },

  "DataDog": {
    "Enabled": false,
    "Debug": false,
    "ServiceName": "building-registry-grb-processor-upload-dev"
  },

  "DistributedLock": {
    "Region": "eu-west-1",
    "TableName": "__DistributedLocks__",
    "LeasePeriodInMinutes": 5,
    "ThrowOnFailedRenew": true,
    "TerminateApplicationOnFailedRenew": true
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Properties": {
      "Application": "BuildingRegistry - Grb Processor Upload",
      "ContainerId": "REPLACE_CONTAINERID"
    }
  }
}
